{
  "type": "module",
  "dependencies": {
    "typescript": "^5.9.3"
  },
  "scripts": {
    "setup_dev": "node .config/setup.js --target development",
    "setup_prod": "node .config/setup.js --target production",
    "build_dev": "npx @11ty/eleventy --input=templates/development.md --output=server/html",
    "build_prod": "npx @11ty/eleventy --input=templates/production.md --output=docs",
    "clasp_push": "cd .config && clasp push",
    "clasp_push_prod": "cd .config && clasp push && clasp deploy --deploymentId AKfycbz9PxglZL_JO524qWuvkW9O1jeOHtlXdHs4IY9xJILTvOvCQXgJttqBEjr9RHiDwntw",
    "deploy_dev": "npm run setup_dev && npm run build_dev && npm run clasp_push",
    "deploy_prod": "npm run setup_prod && npm run build_prod && npm run clasp_push_prod",
    "ts_model": "npx tsc --project tsconfig.json && node data/generate_dances.js"
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "dotenv": "^17.2.3",
    "dotenv-parse-variables": "^2.0.0"
  }
}
